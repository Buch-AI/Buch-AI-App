{
  "name": "buch-ai-app",
  "main": "expo-router/entry",
  "version": "0.1.0",
  "homepage": "http://Buch-AI.github.io/Buch-AI-App",
  "scripts": {
    "start": "expo start --clear --no-dev",
    "reset-project": "node ./scripts/reset-project.js",
    "prepare": "husky",
    "android": "expo start --android --clear --no-dev",
    "ios": "expo start --ios --clear --no-dev --minify",
    "web": "expo start --web --clear --no-dev --minify",
    "web:d": "NODE_ENV=d expo start --web --clear",
    "web:p": "NODE_ENV=p expo start --web --clear --no-dev --minify",
    "test": "jest --watchAll",
    "lint": "eslint ./src",
    "lint:debug": "eslint ./ --debug",
    "lint:fix": "eslint ./ --fix",
    "predeploy": "expo export -p web",
    "deploy": "gh-pages --nojekyll -d dist"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@babel/core": "^7.26.9",
    "@expo/vector-icons": "^14.0.2",
    "@huggingface/inference": "^2.8.1",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "axios": "^1.8.2",
    "expo": "~52.0.36",
    "expo-av": "~15.0.2",
    "expo-blur": "~14.0.3",
    "expo-constants": "~17.0.7",
    "expo-file-system": "~18.0.12",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.17",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.8",
    "expo-web-browser": "~14.0.2",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-markdown-display": "^7.0.2",
    "react-native-reanimated": "^3.16.2",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "^4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "@types/sqlstring": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "eslint": "^8.57.1",
    "eslint-config-expo": "~8.0.1",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.5",
    "lint-staged": "^15.5.1",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.3.3"
  },
  "private": true
}
